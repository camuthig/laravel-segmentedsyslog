{
    "name": "camuthig/laravel-segmentedsyslog",
    "description": "A Laravel Syslog Handler to handle maximum message lengths enforced by certain syslog implementations.",
    "authors": [
        {
            "name": "Chris Muthig",
            "email": "camuthig@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*",
        "monolog/monolog": "~1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9.1"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Camuthig\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
             "tests"
        ]
    },
    "minimum-stability": "stable"
}
